{
  "name": "celluloid",
  "version": "0.1.0",
  "descriptions": "Celluloid helps students and teacher to interact by annotating online educational videos",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "hoist-non-react-statics": "^2.5.0",
    "knex": "^0.15.0",
    "nocache": "^2.0.0",
    "nodemailer": "^4.6.7",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.0",
    "validator": "^10.2.0"
  },
  "scripts": {
    "build": "yarn build-client && yarn build-server && cp -r client/build dist/client",
    "watch": "concurrently \"yarn watch-server\" \"yarn watch-client\"",
    "watch-client": "cd client && yarn start",
    "build-client": "cd client && yarn build",
    "build-server": "webpack --config webpack-server.config.js --colors",
    "watch-server": "concurrently \"yarn build-server --watch\" \"nodemon --use-strict dist/index.js\"",
    "start": "cd dist && node --use-strict index.js",
    "deploy": "./bin/deploy"
  },
  "author": "Erwan Queff√©lec <erwan.queffelec@gmail.com",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/compression": "^0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.0.39",
    "@types/express-session": "^1.15.8",
    "@types/knex": "^0.14.15",
    "@types/nodemailer": "^4.6.2",
    "@types/nodemailer-smtp-transport": "^2.7.4",
    "@types/passport": "^0.4.4",
    "@types/passport-local": "^1.0.33",
    "@types/pg": "^7.4.1",
    "@types/validator": "^9.4.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "concurrently": "^3.5.0",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "nodemon": "^1.11.0",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "typescript": "^2.6.2",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.4"
  }
}
